	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" version="2.2">
	<persistence-unit transaction-type="JTA"
		name="GamifiedMarketingEJB">
		<jta-data-source>GamifiedMarketingDatabase</jta-data-source>
		<class>it.gamified.db2.entities.Answer</class>
		<class>it.gamified.db2.entities.Log</class>
		<class>it.gamified.db2.entities.MarketingAnswer</class>
		<class>it.gamified.db2.entities.MarketingQuestion</class>
		<class>it.gamified.db2.entities.Product</class>
		<class>it.gamified.db2.entities.Questionnaire</class>
		<class>it.gamified.db2.entities.Review</class>
		<class>it.gamified.db2.entities.User</class>
		<class>it.gamified.db2.entities.OffensiveWord</class>
		<properties>
			<property name="eclipselink.logging.level" value="FINE" />
		</properties>
	</persistence-unit>
</persistence>